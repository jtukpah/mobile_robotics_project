<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <param name="robot_description" command="$(find xacro)/xacro.py $(find turtlebot3_description)/urdf/turtlebot3_waffle.urdf.xacro" />

  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->

    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-x 0 -y 0 -z 0 -urdf -model turtlebot3_waffle -param robot_description" output="screen"/>
    <node pkg="turtlebot3_teleop" type="turtlebot3_teleop_key" name="turtlebot3_teleop_keyboard"  output="screen"/>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="rob_st_pub" />
    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state-publisher" />
    <node type="rviz" name="rviz" pkg="rviz" required="true"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find booster1)/worlds/cafe.world"/>
    </include>





</launch>
